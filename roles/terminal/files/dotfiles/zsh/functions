function awsv() {
  aws-vault exec bm-$@
}

function zd() {
  if [[ $# -eq 0 ]]; then
    builtin cd ~ && return
  elif [[ -d "$1" ]]; then
    builtin cd "$1"
  else
    z "$@" && printf "\U000F17A9 " && pwd || echo "Error: Directory not found"
  fi
}

c() { cd ~/Code/$1; }
_c() { _files -W ~/Code -/; }
compdef _c c

bm() {
  "$(git rev-parse --show-toplevel)/bin/bm" "$@"
}
